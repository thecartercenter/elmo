<% items.each do |item, subitems| %>
  <% if item.is_a? QingGroup %>
    <%= render 'qing_groups/printable', qing_group: item, level: level, rank: item_rank(item, rank) %>
    <%= render 'form_items/printable', items: subitems, level: level + 1, rank: item_rank(item, rank) %>
  <% else %>
    <%= render 'questionings/printable', qing: item, level: level, rank: item_rank(item, rank) %>
  <% end %>
<% end %>
